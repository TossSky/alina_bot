{
  "servers": {
    "memory": {
      "enabled": true,
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory", "--storage", "./mcp_data/memory"]
    },
    "filesystem": {
      "enabled": true,
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "./user_data", "./chat_history"]
    },
    "time": {
      "enabled": true,
      "command": "npx",
      "args": ["-y", "@dandeliongold/mcp-time"]
    },
    "sentiment": {
      "enabled": true,
      "url": "http://localhost:5004"
    }
  }
}
